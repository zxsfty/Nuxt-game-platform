{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "strict": true,
    "jsx": "preserve",
    "sourceMap": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "lib": ["ESNext", "DOM"],
    "types": ["vite/client", "node", "@types/webpack", "@nuxt/schema", "@types/node", "vue", "@vue/runtime-core", "@nuxt/types"],
    "skipLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "~/*": ["./*"],
      "@/*": ["./*"],
      "nitropack/types": ["./node_modules/nitropack/types"],
      "#imports": ["./.nuxt/types/nitro-imports"]
    }
  },
  "exclude": ["node_modules", ".nuxt", "dist"]
}